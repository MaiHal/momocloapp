<head>
	<meta charset="utf-8">
	<link rel="stylesheet" href="discography.scss" type="text/css">
</head>
<body>
	<%= form_for(@comment, url: momocloapp_comments_create_path) do |f| %>
    	<div class="create">
    	<p>
            <%= f.label :name, "名前" %><br>
            <%= f.text_field :name %><br>
            <%= f.label :message, "コメント" %><br>
            <%= f.text_area :message %><br>
            <%= f.submit "投稿する"%>
        </p>
        </div>
    <% end %>
</body>